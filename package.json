{
    "scripts": {
        "renameReport": "cd reports && node -e \"require('fs').rename('CHAMBERSAUTOTESTS.html','CHAMBERSAUTOTESTS_%date:~-10,2%_%date:~-7,2%_%date:~-4,4%_%time:~0,2%%time:~3,2%%time:~6,2%.html', function(err) { if (err) console.log(err); console.log('File renamed!') })\""
    },
    "cypress-cucumber-preprocessor": {
        "stepDefinitions": "cypress/e2e/step_definitions/**/*.steps.ts",
        "omitFiltered": true,
        "filterSpecs": true,
        "pretty": {
            "enabled": true
        },
        "json": {
            "enabled": true,
            "output": "jsonlogs/log.json",
            "formatter": "node",
            "args": [
                "./node_modules/@deepakvishwakarma/cucumber-json-formatter/lib/main.js"
            ]
        },
        "html": {
            "enabled": true,
            "output": "./cypress/reports/CHAMBERSAUTOTESTS.html"
        },
        "messages": {
            "enabled": true,
            "output": "jsonlogs/messages.ndjson"
        }
    },
    "dependencies": {
        "@azure/cosmos": "^4.0.0",
        "@azure/service-bus": "^7.9.3",
        "@azure/storage-blob": "^12.17.0",
